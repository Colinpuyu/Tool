{
  "name": "construction-enclosure-calculator",
  "private": true,
  "version": "0.0.0",
  "main": "electron-main.js",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "electron-dev": "electron .",
    "electron-build": "npm run build && electron-builder --win --x64"
  },
  "build": {
    "appId": "com.example.construction-calculator",
    "productName": "建筑围护结构计算器",
    "directories": {
      "output": "release"
    },
    "win": {
      "target": "nsis",
      "icon": "public/icon.ico"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  },
  "dependencies": {
    "uuid": "^13.0.0",
    "react-dom": "^19.2.3",
    "lucide-react": "^0.562.0",
    "react": "^19.2.3"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "@vitejs/plugin-react": "^5.0.0",
    "typescript": "~5.8.2",
    "vite": "^6.2.0"
  }
}
